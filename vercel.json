{
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "ignoreCommand": "git diff --quiet $VERCEL_GIT_COMMIT_REF $VERCEL_GIT_PREVIOUS_SHA . || echo 'changes'",
  "devCommand": "pnpm dev",
  "regions": ["iad1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
